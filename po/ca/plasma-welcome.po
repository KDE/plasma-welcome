#: src/qml/pages/powerfulwhenneeded/MetaKey.qml:18
#, kde-format
msgctxt "@info:window"
msgid "Keyboard Shortcuts"
msgstr "Dreceres de teclat"

#: src/qml/pages/powerfulwhenneeded/MetaKey.qml:23
#, kde-format
